<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>listeners</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <listener>
  	<display-name>ReqListener</display-name>
  	<listener-class>pl.javastart.jdbc.listeners.ReqListener</listener-class>
  </listener>
  
  <listener>
  	<display-name>AttributeListener</display-name>
  	<listener-class>pl.javastart.jdbc.listeners.AttributeListener</listener-class>
  </listener>
  
  <listener>
  	<display-name>DbInitListener</display-name>
  	<listener-class>pl.javastart.jdbc.listeners.DbInitListener</listener-class>
  </listener>
  
  <listener>
  	<display-name>SessionListener</display-name>
  	<listener-class>pl.javastart.jdbc.listeners.SessionListener</listener-class>
  </listener>
</web-app>